<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tuniu.mem.mapper.UserMapper">
    <insert id="insert" parameterType="com.tuniu.mem.bean.User">
        insert into user(id,user_name,email,mobile) values (#{id},#{userName},#{email},#{mobile})
    </insert>
</mapper>